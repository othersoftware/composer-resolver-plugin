{
    "name": "othersoftware/composer-resolver-plugin",
    "description": "Composer plugin to easily manage your dependencies for local development.",
    "license": "MIT",
    "type": "composer-plugin",
    "version": "0.0.1",
    "require": {
        "composer-plugin-api": "^2.0"
    },
    "require-dev": {
        "composer/composer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "OtherSoftware\\Resolver\\": "src/"
        }
    },
    "extra": {
        "class": "OtherSoftware\\Resolver\\Composer\\Resolver"
    }
}
